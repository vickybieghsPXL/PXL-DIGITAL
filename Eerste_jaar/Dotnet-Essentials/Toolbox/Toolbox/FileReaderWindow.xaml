        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Toolbox"
        mc:Ignorable="d"
        Title="FileReaderWindow" Height="222.5" Width="212">
    <Grid>
        <TextBox Name="readTextBox" HorizontalAlignment="Left" Height="108" Margin="10,41,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="108"/>
        <ListBox Name="readListBox" HorizontalAlignment="Left" Height="108" Margin="123,41,0,0" VerticalAlignment="Top" Width="68"/>
        <Label Content="TextBox" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="64"/>
        <Label Content="ListBox" HorizontalAlignment="Left" Margin="123,10,0,0" VerticalAlignment="Top" Width="68"/>
        <Button Name="readButton" Content="Read" HorizontalAlignment="Left" Margin="10,154,0,0" VerticalAlignment="Top" Width="181" Click="readButton_Click" Height="26"/>

    </Grid>
</Window>
